{
    "name": "limingxinleo/mta",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "component"
    ],
    "description": "PHP SDK for Mobile Tencent Analytics (MTA)",
    "authors": [
        {
            "name": "李铭昕",
            "email": "limingxin@swoft.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xin\\Mta\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "pimple/pimple": "^3.2",
        "hyperf/utils": "2.1.*"
    },
    "require-dev": {
        "swoft/swoole-ide-helper": "dev-master",
        "phpunit/phpunit": ">=5.7",
        "phpstan/phpstan": "^0.12",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "cs-fix": "php-cs-fixer fix $1",
        "analyse": "phpstan analyse src"
    }
}
